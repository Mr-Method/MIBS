           ::= { imabGroupIntervalTable 1 }

imabGroupIntervalEntry ::= SEQUENCE {
           imabGroupIntervalNumber                   INTEGER (1.. 96),
           imabGroupIntervalUnavailSecs          Gauge32,
           imabGroupIntervalNeNumFailures      Gauge32,
           imabGroupIntervalFeNumFailures      Gauge32 }

imabGroupIntervalNumber OBJECT-TYPE
           SYNTAX            INTEGER (1.. 96)
           MAX-ACCESS    not-accessible
           STATUS           current
           DESCRIPTION
              "A number between 1 and 96, where 1 is the most
                recently completed 15 minute interval and 96 is the least
                recently completed 15 minutes interval (assuming that all
                96 intervals are valid)."
