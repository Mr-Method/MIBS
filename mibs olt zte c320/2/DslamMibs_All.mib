	REFERENCE
		"ATM Forum IMA v1.1, Section 5.2.2.1 on page 26"
::= { imaLinkEntry 11 }

imaLinkScramble OBJECT-TYPE
	SYNTAX        INTEGER{
			disable(0),
			 enable(1)
	}
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION
		"enable/disable the scramble status of the link"
::= { imaLinkEntry 12 }

