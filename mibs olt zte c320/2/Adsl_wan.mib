    ETHERPORT-MANAGER-MIB DEFINITIONS ::= BEGIN

    IMPORTS
        MODULE-IDENTITY, OBJECT-TYPE,
        Integer32, Counter64                    FROM SNMPv2-SMI
        
        DisplayString, TruthValue               FROM SNMPv2-TC
        
        MODULE-COMPLIANCE, OBJECT-GROUP         FROM SNMPv2-CONF
        
        PortList				FROM Q-BRIDGE-MIB
        
        zxEtherBoard                            FROM WRS-MASTER-MIB;       
       
        etherIfaceManager     			OBJECT IDENTIFIER ::= { zxEtherBoard 3 }


    etherIfaceManagerTable OBJECT-TYPE
        SYNTAX SEQUENCE OF EtherIfaceManagerEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table of switch interfaces and associated properties."
        ::= { etherIfaceManager  1 }

    etherIfaceManagerEntry OBJECT-TYPE
        SYNTAX EtherIfaceManagerEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Table entry for switch interface control and status information."
        INDEX { etherIfaceUnit, etherIfacePort }
        ::= { etherIfaceManagerTable 1 }

    EtherIfaceManagerEntry ::= SEQUENCE {
        etherIfaceUnit                Integer32,
        etherIfacePort                Integer32,
        etherIfaceLLWHPort            Integer32,
        etherIfaceIndex               Integer32,
        etherIfaceName                DisplayString,
        etherIfaceEnable              TruthValue,
        etherIfaceLink                INTEGER,
        etherIfacePriority            INTEGER,
        etherIfaceBroadCast           INTEGER ,
        etherIfaceCreditFlag          INTEGER ,
        etherIfaceMultSwitch 	      INTEGER,
        etherIfaceUseProfilId         INTEGER, 
        etherIfacePermitMultGrpNum    INTEGER  ,
        etherIfaceAuthIpSwitch        INTEGER  ,
        etherIfaceAuthMacSwitch       INTEGER  ,
        etherIfaceMirrorSwich         INTEGER  ,
        etherIfaceMirrorMode          INTEGER  ,
        etherIfaceMirrorPortList      PortList ,
        etherIfacelinkScan            INTEGER  ,     
        etherIfaceDuplex	      INTEGER  ,
        etherIfaceSpeed               INTEGER  ,
        etherIfaceAutoStatus 	      INTEGER  ,
        
        etherIfaceMulticastDegree       Integer32,
        etherIfaceUnUnicastDegree       Integer32,
        etherIfaceFloodUnUnicastFlag  	INTEGER  ,
        etherIfaceFloodUnMulticastFlag  INTEGER  ,
        etherIfaceFloodBroadcastFlag  	INTEGER  
    }

    etherIfaceUnit  OBJECT-TYPE
        SYNTAX Integer32 (1..12)
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "An index that uniquely identifies a unit in the TMS galnet
             stack.  If an invalid value is used for the index, a SNMP
             'noSuchName' error (SNMPv1) or 'noSuchInstance' exception
             (SNMPv2/v3) is returned.  For implementations that do not support
             stacking, the same response is returned for any unit index other
             than 1."
        ::= { etherIfaceManagerEntry 1 }

    etherIfacePort OBJECT-TYPE
        SYNTAX Integer32 (1..8)
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "An index that uniquely identifies a TMS Application 'logical port'
             (i.e., IEEE 802.3ad Aggregator) within the oemArchIfaceUnit."
        ::= { etherIfaceManagerEntry 2 }

    etherIfaceLLWHPort OBJECT-TYPE
        SYNTAX Integer32 (8193..8296)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "A value that uniquely identifies a TMS Application 'logical port'
             (i.e., IEEE 802.3ad Aggregator) in the TMS galnet stack.  This is
             a 'Layered Linear With Holes' (LLWH) port number that may may have
             holes for missing ports or units.  For this MIB, 26 port numbers
             are reserved for each unit in the stack.  For example, LLHW port
             8193 is unit 1, port 1 and LLHW 8228 is unit 2, port 10."
        ::= { etherIfaceManagerEntry 3 }

    etherIfaceIndex OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The ifIndex of this TMS Application 'logical port' (i.e., IEEE
             802.3ad Aggregator).  Note that an ifIndex value of 34603009 
             (0x02100001) represents the Aggregator layer, unit 1, slot 0
             (base unit ports), port 1."
        ::= { etherIfaceManagerEntry 4 }

    etherIfaceName OBJECT-TYPE
        SYNTAX DisplayString (SIZE (0..16))
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "DURABLE:
             The textual name of this interface, e.g., 'John'."
        ::= { etherIfaceManagerEntry 5 }

    etherIfaceEnable OBJECT-TYPE
        SYNTAX TruthValue
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "DURABLE: { true:all }
             This object is true(1) when this interface is enabled and
             false(1) when it is disabled.  For this product, this is the
             ONLY way to enable or disable this interface.  Note that
             'ifAdminStatus' in RFC1213 and RFC2233 and 'dot1dStpPortEnable'
             in RFC1493 are each implemented as 'read-only'."
        ::= { etherIfaceManagerEntry 6 }

    etherIfaceLink OBJECT-TYPE
        SYNTAX TruthValue
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "DURABLE: { true:all }
             This object is true(1) when Spanning Tree operation is enabled
             for this interface and false(2) when it is disabled."
        ::= { etherIfaceManagerEntry 7 }

     etherIfacePriority OBJECT-TYPE
        SYNTAX INTEGER (0..7)
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            " "
        ::= { etherIfaceManagerEntry 8 } 
        
     etherIfaceBroadCast    OBJECT-TYPE
     SYNTAX INTEGER(0..4)
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            " forbit broadcast or not
             0--0k;
             1--128k;
             2--512k;
             3--1024k;
             4--no limit;
            "
        ::= { etherIfaceManagerEntry 9 }       
            
      etherIfaceCreditFlag  OBJECT-TYPE
       SYNTAX INTEGER{
               assure(1),           
               unassured(2)
        	}
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
              " port assure switch"
        ::= { etherIfaceManagerEntry 10 } 
         
      etherIfaceMultSwitch 	  OBJECT-TYPE
	  SYNTAX      INTEGER{
	  		enable(1),
	  		disable(2)
	  		}
	  MAX-ACCESS  read-write
	  STATUS      current
	  DESCRIPTION
	      "port multicast swtich"
	  REFERENCE
	        ""
	    ::= { etherIfaceManagerEntry 11}
    
      etherIfaceUseProfilId   OBJECT-TYPE
	  SYNTAX      INTEGER(1..256)
	  MAX-ACCESS  read-write
	  STATUS      current
	  DESCRIPTION
	      "port using profile index "
	  REFERENCE
	        ""
	    ::= { etherIfaceManagerEntry 12}
           
      etherIfacePermitMultGrpNum    OBJECT-TYPE
	  SYNTAX      INTEGER(0..16)
	  MAX-ACCESS  read-write
	  STATUS      current
	  DESCRIPTION
	      "port permit multicast num "
	  REFERENCE
	        ""
    ::= { etherIfaceManagerEntry 13}
    
      etherIfaceAuthIpSwitch  OBJECT-TYPE
	  SYNTAX      INTEGER{
	               enable(1),           
	               disable(2)
	             }
	  MAX-ACCESS  read-write
	  STATUS      current
	  DESCRIPTION
	      ""
	  REFERENCE
        ""
    ::= { etherIfaceManagerEntry 14}  
    
      etherIfaceAuthMacSwitch    OBJECT-TYPE
	  SYNTAX      INTEGER{
	               enable(1),           
	               disable(2)
	             }
	  MAX-ACCESS  read-write
	  STATUS      current
	  DESCRIPTION
	      ""
	  REFERENCE
        ""
    ::= { etherIfaceManagerEntry 15}     
    
    etherIfaceMirrorSwich  OBJECT-TYPE
	  SYNTAX      INTEGER
	  MAX-ACCESS  read-write
	  STATUS      current
	  DESCRIPTION
	      ""
	  REFERENCE
              "1:enable;
               2:disable"
    ::= { etherIfaceManagerEntry 16}   
    
    etherIfaceMirrorMode OBJECT-TYPE
	  SYNTAX      INTEGER(0..1)
	  MAX-ACCESS  read-write
	  STATUS      current
	  DESCRIPTION
	      ""
	  REFERENCE
              "1:ingress;
               2:engress
               "
    ::= { etherIfaceManagerEntry 17}   
    
    etherIfaceMirrorPortList  OBJECT-TYPE
	  SYNTAX      PortList
	  MAX-ACCESS  read-write
	  STATUS      current
	  DESCRIPTION
	      ""
	  REFERENCE
              ""
    ::= { etherIfaceManagerEntry 18}   
    
    etherIfacelinkScan OBJECT-TYPE
	  SYNTAX      INTEGER(1..3)
	  MAX-ACCESS  read-write
	  STATUS      current
	  DESCRIPTION
	      ""
	  REFERENCE
              ""
    ::= { etherIfaceManagerEntry 19}  
    
    etherIfaceDuplex OBJECT-TYPE
        SYNTAX INTEGER {
             autonegotiate(1),
             half(2),
             full(3),
             illegal(99)
        }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "DURABLE: { autonegotiate:all }
            The desired speed and duplex for this interface. If the selected
            control is not possible on the interface, a value of illegal(99)
            is returned.  If the port type does NOT support the default
            of autonegotiate(1), then the application initializes the
            port to a valid value (e.g., 1000full(6)).  Note that not all
            controls are possible for all interfaces.  For example, only
            full-1000(6) is available for Gigabit Ethernet interfaces."
        ::= { etherIfaceManagerEntry 20 }    
    
    etherIfaceSpeed     OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "DURABLE: { autonegotiate:all }
            The desired speed and duplex for this interface. If the selected
            control is not possible on the interface, a value of illegal(99)
            is returned.  If the port type does NOT support the default
            of autonegotiate(1), then the application initializes the
            port to a valid value (e.g., 1000full(6)).  Note that not all
            controls are possible for all interfaces.  For example, only
            full-1000(6) is available for Gigabit Ethernet interfaces.
            for:autonegotiate(1)/10(2)/100(3)/illegal(4)"
        ::= { etherIfaceManagerEntry 21}        
        
   etherIfaceAutoStatus      OBJECT-TYPE
   SYNTAX INTEGER{
               enable(1),           
               disable(2)
             }
            
  MAX-ACCESS  read-write
  STATUS      current
  DESCRIPTION
      " disable/enable the port receive broad packet"
