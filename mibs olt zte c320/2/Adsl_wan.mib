      "the learning macaddresses of every pvc port"
  REFERENCE
        ""
    ::= { pvcManagerEntry 4}

wanPvcPortForwBroad OBJECT-TYPE
 SYNTAX INTEGER {
               enable(1),           
               disable(2)
             }
            
  MAX-ACCESS  read-write
