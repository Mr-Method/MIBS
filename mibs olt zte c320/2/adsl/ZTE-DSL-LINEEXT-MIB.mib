-- Dsl Loop Test Table
    zxDslLoopTestTable  OBJECT-TYPE
            SYNTAX      SEQUENCE OF ZxDslLoopTestEntry
            MAX-ACCESS  not-accessible
            STATUS      current
            DESCRIPTION
                 "This table provides one row for each DSL line pvc tesing."
            ::= {zxAdslExtMibObjects 9}
    
    zxDslLoopTestEntry  OBJECT-TYPE
            SYNTAX      ZxDslLoopTestEntry
            MAX-ACCESS  not-accessible
            STATUS      current
            DESCRIPTION
                "DSL line pvc tesing information entry"
            INDEX    { zxDslLoopTestPort, zxDslLoopTestPvcNo }
            ::= {zxDslLoopTestTable 1}
    
    ZxDslLoopTestEntry ::=
            SEQUENCE {
                zxDslLoopTestPort               INTEGER,
                zxDslLoopTestPvcNo              INTEGER,
                zxDslLoopTestType               INTEGER,
                zxDslLoopTestOperStatus         INTEGER,
                zxDslLoopTestResult             INTEGER
                }
    
    zxDslLoopTestPort  OBJECT-TYPE
            SYNTAX      INTEGER
            MAX-ACCESS  not-accessible
            STATUS      current
            DESCRIPTION
                 "Dsl port number.It should be equal to ifIndex in DSL layer."
            ::= { zxDslLoopTestEntry 1 }
            
    zxDslLoopTestPvcNo  OBJECT-TYPE
            SYNTAX      INTEGER
            MAX-ACCESS  not-accessible
            STATUS      current
            DESCRIPTION
                 "Dsl port pvc number.If it is 65535,it means this test doesn't 
            base on pvc but physical port."
            ::= { zxDslLoopTestEntry 2 }

    zxDslLoopTestType  OBJECT-TYPE
            SYNTAX      INTEGER {
                noOper(0),  -- No operation or delete loopback point.
                cancle(1),  -- Cancle this test
                
                utopia(2),  -- ATUC Utopia loopback test
                afe(3),     -- ATUC AFE loopback test
                hybrid(4),  -- ATUC Hybrid loopback test
                atuc_OAM(5),-- ATUC data path loopback test,such as ATM OAM F5 loopback.
                atur_OAM(6),-- ATUR data path loopback test,such as ATM OAM F5 loopback.
                atur_CC(7), -- ATUR ATM continuity cell test.
                digital(8)  -- ATUC DSP module loopback test.
                }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                 "This is loop test action expected."
            ::= { zxDslLoopTestEntry 3 }
            
    zxDslLoopTestOperStatus  OBJECT-TYPE
            SYNTAX      INTEGER {
                neverExecute(0),   -- Never execute test
                executing(1),      -- Executing test now
                executed(2),       -- Test has been executed
                failedToCommit(3) -- Failed to commit test command to DSL card.
                }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "This is loop test operational status.
                  
                  Note: if zxDslLoopTestOperStatus equal to Executing(1),
                  ifOperStaus should be tesing(3). "
            ::= { zxDslLoopTestEntry 4 }

    zxDslLoopTestResult  OBJECT-TYPE
            SYNTAX      INTEGER {
                noResult(0), 
                success(1),
                generalFailed(2),  -- Test failed but no detailed reason.                 
                noSupport(3),      -- Current DSL card doesn't support this test.         
                unknown(4),        -- Unkown the test result.                             
                noSuchPort(5),     -- No this port on dsl card                            
                loopBackFailed(6), -- Loop back failed on dsl card                        
                portNotActive(7),  -- Some test can't be executed when port is not actived. 
                portInTesting(8),  -- Port in testing and can't execute the new test            
                portInService(9),  -- Port in service and can't execute the test                
                portFailures(10),  -- Port has failures and can't execute the test             
                cardFailures(11),  -- Dsl card has failures and can't execute the test
                noPvcFound(12),    -- No PVC found on the test port or VPI/VCI is not 8/81 while test type is ATUR-OAM
                unknownTestType(13)-- The test type is not defined in zxDslLoopTestType

                }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "This is loop test result and includes reasons for test failures."
            ::= { zxDslLoopTestEntry 5 }
            



