::= { imaLinkIntervalTable 1 }

ImaLinkIntervalEntry ::= SEQUENCE {
		imaLinkIntervalNumber 		 	INTEGER,
	   	imaLinkIntervalImaViolations 	Gauge32,
   		imaLinkIntervalOifAnomalies 	Gauge32,
    	imaLinkIntervalTxStuffs 		Gauge32,
		imaLinkIntervalRxStuffs 		Gauge32,
   		imaLinkIntervalTxUserCells  	Gauge32, 
		imaLinkIntervalRxUserCells  	Gauge32, 
		imaLinkIntervalFilteredCells 	Gauge32, 
		imaLinkIntervalDroppedCells 	Gauge32, 
		imaLinkIntervalIcpCells  		Gauge32, 
