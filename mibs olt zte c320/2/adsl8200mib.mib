zxDslamCardUp	NOTIFICATION-TYPE
    OBJECTS   {  zxDslamCardOperStatus }
    STATUS    current
    DESCRIPTION 
		"A cardUp trap signifies that the SNMP agent has detectd 
		that the cardOperStatus object for the card has transitioned 
		out of the down state.This trap indicates that some card has 
		recovered from fault or been inserted into one switch slot"
     ::= { zxDslamTrap 0 1 }

zxDslamCardDown 	NOTIFICATION-TYPE
    OBJECTS   { zxDslamCardOperStatus }
    STATUS    current
    DESCRIPTION	
		"A cardDown trap signifies that the SNMP agent has deected that 
		the cardOperStatus object for the card has transitioned into 
		the down state. This trap indicates that some card has occured 
		fault or been pulled out from one switch slot."
     ::= { zxDslamTrap 0 2 }
     
zxDslamCommandTabStatusLastChange NOTIFICATION-TYPE
    OBJECTS   { zxDslamCommandStatus }
    STATUS    current
    DESCRIPTION	
		"This trap signifies that the SNMP agent has detected that 
		the zxDslamCommandStatus object for the command has been changed."
     ::= { zxDslamTrap 0 3 }

zxDslamCoreStandbyStatusChange NOTIFICATION-TYPE
    OBJECTS   { zxDslamCardstandbyStatus }
    STATUS    current
    DESCRIPTION	
		"This trap signifies that the SNMP agent has detected that the	
		zxDslamCardstandbyStatus object for the core card has been changed."
     ::= { zxDslamTrap 0 4 }
     
zxDslamFanOperStatusChange NOTIFICATION-TYPE
    OBJECTS   { zxDslamFanOperStatus}
    STATUS    current
    DESCRIPTION	
		"This trap signifies that the SNMP agent has detected that the	
		zxDslamFanOperStatus object for the fan has been changed."
     ::= { zxDslamTrap 0 5 }
     
zxDslamPowerOperStatusChange NOTIFICATION-TYPE
