-- li     ::= { dot1qVlanCurrentEntry 2 }
-- li 
-- li dot1qVlanFdbId OBJECT-TYPE
-- li     SYNTAX      Unsigned32
-- li     MAX-ACCESS  read-only
-- li     STATUS      current
-- li     DESCRIPTION
-- li         "The Filtering Database used by this VLAN.  This is one
-- li         of the dot1qFdbId values in the dot1qFdbTable.  This
-- li         value is allocated automatically by the device whenever
-- li         the VLAN is created: either dynamically by GVRP, or by
-- li 
-- li         management, in dot1qVlanStaticTable.  Allocation of this
-- li         value follows the learning constraints defined for this
-- li         VLAN in dot1qLearningConstraintsTable."
-- li     ::= { dot1qVlanCurrentEntry 3 }
-- li 
-- li dot1qVlanCurrentEgressPorts OBJECT-TYPE
-- li     SYNTAX      PortList
-- li     MAX-ACCESS  read-only
-- li     STATUS      current
-- li     DESCRIPTION
-- li         "The set of ports which are transmitting traffic for
-- li         this VLAN as either tagged or untagged frames."
-- li     REFERENCE
-- li         "IEEE 802.1Q/D11 Section 12.10.2.1"
-- li     ::= { dot1qVlanCurrentEntry 4 }
-- li 
-- li dot1qVlanCurrentUntaggedPorts OBJECT-TYPE
-- li     SYNTAX      PortList
-- li     MAX-ACCESS  read-only
-- li     STATUS      current
-- li     DESCRIPTION
-- li         "The set of ports which are transmitting traffic for
-- li         this VLAN as untagged frames."
-- li     REFERENCE
-- li         "IEEE 802.1Q/D11 Section 12.10.2.1"
