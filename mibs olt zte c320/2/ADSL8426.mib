-- li     ::= { dot1qForwardAllEntry 1 }
-- li 
-- li dot1qForwardAllStaticPorts OBJECT-TYPE
-- li     SYNTAX      PortList
-- li     MAX-ACCESS  read-write
-- li     STATUS      current
