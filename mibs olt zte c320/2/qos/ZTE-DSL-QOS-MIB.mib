    DESCRIPTION  "The DSCP value of VoIP media."
    DEFVAL       { 0 }
    ::=  {  zxDslQosGlobalTable 14 }  
  
  zxDslQosClassTable  OBJECT-TYPE
    SYNTAX    SEQUENCE  OF  ZxDslQosClassEntry
    MAX-ACCESS  not-accessible
    STATUS    current
    DESCRIPTION "The (conceptual) table listing all QoS class."
    ::=  { zxDslQosClass  1 }

  zxDslQosClassEntry  OBJECT-TYPE
    SYNTAX    ZxDslQosClassEntry
    MAX-ACCESS  not-accessible
    STATUS    current
    DESCRIPTION "An entry (conceptual row) representing a QoS class."
    INDEX   {  zxDslQosClassName  }
    ::=  { zxDslQosClassTable 1 }

  ZxDslQosClassEntry  ::=  SEQUENCE {
    zxDslQosClassName  DisplayString,
    zxDslQosClassMatch  DisplayString,
    zxDslQosClassStatus  RowStatus
    }


  zxDslQosClassName OBJECT-TYPE 
    SYNTAX      DisplayString  ( SIZE ( 0 .. 32  ) ) 
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION   "The name of this QoS class
            referred to in this table entry."
    ::=  {  zxDslQosClassEntry  1  }


  zxDslQosClassMatch  OBJECT-TYPE 
    SYNTAX      DisplayString  ( SIZE ( 0 .. 255  ) ) 
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION   "The string contains one class's qualification and its meansing is defined before."
    ::=  {  zxDslQosClassEntry  2  }


  zxDslQosClassStatus OBJECT-TYPE 
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION   "RowStatus"
    ::=  {  zxDslQosClassEntry  3  }

  zxDslQosPolicyTable OBJECT-TYPE
    SYNTAX    SEQUENCE  OF  zxDslQosPolicyEntry
    MAX-ACCESS  not-accessible
    STATUS    current
    DESCRIPTION "The (conceptual) table listing all QoS policy."
    ::=  { zxDslQosPolicy  1 }

  zxDslQosPolicyEntry OBJECT-TYPE
    SYNTAX    zxDslQosPolicyEntry
    MAX-ACCESS  not-accessible
    STATUS    current
    DESCRIPTION ""
    INDEX   {  zxDslQosPolicyName  }
    ::=  { zxDslQosPolicyTable 1 }

  zxDslQosPolicyEntry  ::=  SEQUENCE {
    zxDslQosPolicyName  DisplayString,
    zxDslQosClassOrder  Integer32,
    zxDslQosPolicyRowStatus  RowStatus
    }


  zxDslQosPolicyName  OBJECT-TYPE 
    SYNTAX      DisplayString  ( SIZE ( 0 .. 32  ) ) 
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION   "The name of this QoS policy
                referred to in this table entry."
    ::=  {  zxDslQosPolicyEntry  1  }


  zxDslQosClassOrder  OBJECT-TYPE 
    SYNTAX      INTEGER  { restricted ( 1 ) , unrestricted ( 2 ) } 
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION   "The index of this QoS policy
                          referred to in this table entry."
    ::=  {  zxDslQosPolicyEntry  2  }


  zxDslQosPolicyRowStatus OBJECT-TYPE 
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION   ""
    ::=  {  zxDslQosPolicyEntry  3  }

  zxDslQosPolicyConfigTable OBJECT-TYPE
    SYNTAX    SEQUENCE  OF  zxDslQosPolicyConfigEntry
    MAX-ACCESS  not-accessible
    STATUS    current
    DESCRIPTION "The (conceptual) table listing all QoS policy."
    ::=  { zxDslQosPolicy  2 }

  zxDslQosPolicyConfigEntry OBJECT-TYPE
    SYNTAX    zxDslQosPolicyConfigEntry
    MAX-ACCESS  not-accessible
    STATUS    current
    DESCRIPTION "An entry (conceptual row) representing a QoS policy."
    INDEX   {  zxDslQosPolicyName, zxDslQosClassName  }
    ::=  { zxDslQosPolicyConfigTable 1 }

  zxDslQosPolicyConfigEntry  ::=  SEQUENCE {
    zxDslQosPolicyAction                INTEGER,
    zxDslQosPolicyRateLimit             Integer32,
    zxDslQosPolicyBurst                 Integer32,
    zxDslQosPolicyExceedAction          Integer32,
    zxDslQosPolicyExceedActValue        Integer32,
    zxDslQosPolicyActionCos             Integer32,
    zxDslQosPolicyActionDSCP            Integer32,
    zxDslQosPolicyActionVLAN            Integer32,
    zxDslQosPolicyActionRedirectedTo    Integer32,
    zxDslQosPolicyActionMirroredTo      Integer32,
    zxDslQosPolicyActionISStatistics    Integer32,
    zxDslQosPolicyStatus                RowStatus,
    zxDslQosPolicyClassSerial           Integer32,
    zxDslQosPolicyPir                   Integer32,
    zxDslQosPolicyPbs                   Integer32,
    zxDslQosPolicyActionCCos            Integer32,
    zxDslQosPolicyTrTcmExceedAction     INTEGER,
    zxDslQosPolicyActionTrafficClass    Integer32
    }


  zxDslQosPolicyAction  OBJECT-TYPE
    SYNTAX      INTEGER  { permit ( 1 ) , deny ( 2 ) } 
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION   "If it is enabled, the flow will be denied"
    ::=  {  zxDslQosPolicyConfigEntry  1  }


  zxDslQosPolicyRateLimit OBJECT-TYPE 
    SYNTAX      Integer32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION   "the config rate of the corresponding token barrel of the class."
    ::=  {  zxDslQosPolicyConfigEntry  2  }


  zxDslQosPolicyBurst OBJECT-TYPE 
    SYNTAX      Integer32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION   "the config burst size of the corresponding token barrel of the class."
    ::=  {  zxDslQosPolicyConfigEntry  3  }


  zxDslQosPolicyExceedAction  OBJECT-TYPE 
    SYNTAX      INTEGER  { drop ( 1 ) , SetDSCP ( 2 ), SetCos ( 3 )} 
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION   "The mib means the acion would be taken if the flow of the class exceeds 
      the config rate. And if the acion is 1(drop),it means the flow will be droped at all.
      However the following mib qoSPolicyExceedActValue has no meaning .
      And if the action is 2(SetDSCP),it means the flow will be changed the DSCP value and 
      the wanted DSCP value is stored in mib qoSPolicyExceedActValue."
    ::=  {  zxDslQosPolicyConfigEntry  4  }


  zxDslQosPolicyExceedActValue  OBJECT-TYPE 
    SYNTAX      Integer32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION   "if the qoSPolicyExceedAction mib = 2(SetDSCP) ,the mib's meaning is the value wanted."
    ::=  {  zxDslQosPolicyConfigEntry  5  }


  zxDslQosPolicyActionCos OBJECT-TYPE 
    SYNTAX      Integer32  ( 0 .. 7  ) 
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION   "If the flow is classified, the action can be changed the COS value in the flow ."
    ::=  {  zxDslQosPolicyConfigEntry  6  }


  zxDslQosPolicyActionDSCP  OBJECT-TYPE 
    SYNTAX      Integer32  ( 0 .. 63  ) 
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION   "If the flow is classified, the action can be changed the DSCP value in the flow ."
    ::=  {  zxDslQosPolicyConfigEntry  7  }


  zxDslQosPolicyActionVLAN  OBJECT-TYPE 
    SYNTAX      Integer32  ( 0 .. 2048  ) 
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION   "If the flow is classified, the action can be changed the Vlan ID value in the flow ."
    ::=  {  zxDslQosPolicyConfigEntry  8  }


  zxDslQosPolicyActionRedirectedTo  OBJECT-TYPE 
    SYNTAX      Integer32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION   "If the flow is classified, the action can be changed the Port value in the flow ."
    ::=  {  zxDslQosPolicyConfigEntry  9  }


  zxDslQosPolicyActionMirroredTo  OBJECT-TYPE 
    SYNTAX      Integer32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION   "If the flow is classified, the action can be mirrored."
    ::=  {  zxDslQosPolicyConfigEntry  10  }


  zxDslQosPolicyActionISStatistics  OBJECT-TYPE 
    SYNTAX      INTEGER  { enable ( 1 ) , disable ( 2 ) } 
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION   "If it is enabled, the flow's statistics will be started"
    ::=  {  zxDslQosPolicyConfigEntry  11  }


  zxDslQosPolicyStatus  OBJECT-TYPE 
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION   "RowStatus"
    ::=  {  zxDslQosPolicyConfigEntry  12  }


  zxDslQosPolicyClassSerial OBJECT-TYPE 
    SYNTAX      Integer32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION   ""
    ::=  {  zxDslQosPolicyConfigEntry  13  }


  zxDslQosPolicyPir OBJECT-TYPE 
    SYNTAX      Integer32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION   ""
    ::=  {  zxDslQosPolicyConfigEntry  14  }


  zxDslQosPolicyPbs OBJECT-TYPE     
    SYNTAX      Integer32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION   ""
    ::=  {  zxDslQosPolicyConfigEntry  15  }
  
  zxDslQosPolicyActionCCos OBJECT-TYPE 
    SYNTAX      Integer32  ( 0 .. 7  ) 
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION   
        "If the flow is classified, the action can be changed the CCOS value
         in the flow ."
    ::=  {  zxDslQosPolicyConfigEntry  16  }
  
  zxDslQosPolicyTrTcmExceedAction OBJECT-TYPE 
    SYNTAX      INTEGER
        {
            dropYellow(1),
            forwardRed(2)
        }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "TrTCM means Two Rate Three Color Marker. The mib means the action 
         would be taken if the flow of the class exceeds the traffic limit. 
        "
    DEFVAL  {  dropYellow  }
    ::=  {  zxDslQosPolicyConfigEntry  17  }
    
  zxDslQosPolicyActionTrafficClass OBJECT-TYPE 
    SYNTAX      Integer32 ( 0 .. 255|4008636142)
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "If the flow is classified, the action can change the traffic class
         value in the IPv6 flow. 
         
         The value 0xeeeeeeee means that the parameter is not configured.
        "
    ::=  {  zxDslQosPolicyConfigEntry  18  } 
    

