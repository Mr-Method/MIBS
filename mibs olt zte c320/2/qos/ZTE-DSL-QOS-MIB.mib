        SYNTAX      OBJECT IDENTIFIER
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION   
            "Logical id of this port. This object is filled by different value
             according to zxDslQosTrafficVCircuitType:
             physicalPort(1) - filled by 0;
             bridgePort(2)   - filled by bridge port number;
             vlan(3)         - filled by svlanId.cvlanId;
            "
        ::=  {  zxDslQosTrafficIfCfgEntry  6  }
        
    zxDslQosTrafficProfile  OBJECT-TYPE
        SYNTAX      DisplayString(1..32)
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The traffic profile."
        ::= { zxDslQosTrafficIfCfgEntry 7 } 
    
    zxDslQosTrafficIfCfgRowStatus  OBJECT-TYPE 
        SYNTAX      RowStatus
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION   
            "This object is used to create a new row or delete an existing
             row in this table."
        ::=  { zxDslQosTrafficIfCfgEntry  20 }
        
-- ---------------------------------------------------------------------------
--  14.zxDslQosCompatibility
-- ---------------------------------------------------------------------------        
  
   zxDslQosCompatible  OBJECT-TYPE 
      SYNTAX      OCTET STRING (SIZE (4))
      MAX-ACCESS  read-only
      STATUS      current
      DESCRIPTION   
          "The compatibility of DSLAM. Every bit denotes the compatibility of 
           some function. If the value of the bit is 1, the related function
           has compatible problem. The related function of the first bit is the
           naming rules of string.
                       
           bit 1: value 1 means support zxDslQosPolicyActionTrafficClass in
                  zxDslQosPolicyConfigTable.
          "
      ::=  {  zxDslQosCompatibility  1  }
END
