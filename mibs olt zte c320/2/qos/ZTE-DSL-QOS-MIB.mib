    ::=  {  zxDslQosGlobalTable  7  }        
      
  zxDslQosNetWorkSideQueueThreshold  OBJECT-TYPE 
    SYNTAX      Integer32  ( 64 .. 128  ) 
    UNITS       "kbytes"
    MAX-ACCESS  read-write
    STATUS      current
