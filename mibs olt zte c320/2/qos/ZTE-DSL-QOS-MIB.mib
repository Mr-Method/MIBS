  zxDslQosPolicy          OBJECT IDENTIFIER   ::=  {  zxDslQosObjects  3  }
  zxDslQosMapping         OBJECT IDENTIFIER   ::=  {  zxDslQosObjects  4  }
  zxDslQosScheduler       OBJECT IDENTIFIER   ::=  {  zxDslQosObjects  5  }
  zxQueuing               OBJECT IDENTIFIER   ::=  {  zxDslQosObjects  6  }
  zxDslQosPortPolicy      OBJECT IDENTIFIER   ::=  {  zxDslQosObjects  7  }
  zxDslQosPortInfo        OBJECT IDENTIFIER   ::=  {  zxDslQosObjects  8  }
  zxDslQosStat            OBJECT IDENTIFIER   ::=  {  zxDslQosObjects  9  }
  zxDslPortMirror         OBJECT IDENTIFIER   ::=  {  zxDslQosObjects  10 }
  zxDslVInterface         OBJECT IDENTIFIER   ::=  {  zxDslQosObjects  11 }              
  zxDslQosPvcPolicy       OBJECT IDENTIFIER   ::=  {  zxDslQosObjects  12 }
  zxDslProtocolVlanMap    OBJECT IDENTIFIER   ::=  {  zxDslQosObjects  13 }
  zxDslQosTraffic         OBJECT IDENTIFIER   ::=  {  zxDslQosObjects  14 }
  zxDslQosCompatibility   OBJECT IDENTIFIER   ::=  {  zxDslQosObjects  50 }

  zxDslQosTrustPriMode  OBJECT-TYPE 
    SYNTAX      INTEGER  { none ( 0 ) , cos ( 1 ) , dscp ( 2 ) } 
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION   "The type of trust PRI"
    ::=  {  zxDslQosGlobalTable  1  }

  zxDslQosPriMapEnable  OBJECT-TYPE 
    SYNTAX      INTEGER  { map ( 1 ) , no_Map ( 2 ) } 
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION   "The type of map"
    ::=  {  zxDslQosGlobalTable  2  }
    
  zxDslQosNetWorkSideQueueNum OBJECT-TYPE 
    SYNTAX      INTEGER  { one(1), two(2), four(4),  eight(8) } 
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION   "The number of networkside queue"
    ::=  {  zxDslQosGlobalTable  3  }
  
  zxDslQosUserSideQueueNum  OBJECT-TYPE 
    SYNTAX      INTEGER  { one(1), two(2), four(4),  eight(8) } 
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION   "The number of userside queue"
    ::=  {  zxDslQosGlobalTable  4  }

  zxDslQosVoipCos OBJECT-TYPE 
