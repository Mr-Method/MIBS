    zxDslProtocolVlanMapTable OBJECT-TYPE
        SYNTAX      SEQUENCE  OF  ZxDslProtocolVlanMapEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION  
            "ethernet protocol vlan mapping."
        ::=  { zxDslProtocolVlanMap  1 }

    zxDslProtocolVlanMapEntry  OBJECT-TYPE
        SYNTAX      ZxDslProtocolVlanMapEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION  
            "ethernet protocol vlan mapping."
        INDEX  { ifIndex, zxDslQosPvcNumber, zxDslEtherProtocolType }
        ::=  { zxDslProtocolVlanMapTable 1 }    
             
    ZxDslProtocolVlanMapEntry  ::=  SEQUENCE {
        zxDslEtherProtocolType           Integer32,
        zxDslProtocolSVlanId             Integer32,
        zxDslProtocolSVlanPriority       Integer32,
        zxDslProtocolCVlanId             Integer32,
        zxDslProtocolCVlanPriority       Integer32,
        zxDslProtocolVlanRowStatus       RowStatus
    }           
  
    zxDslEtherProtocolType OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            " zxDslEtherProtocolType uses a complex coding to support 
              three different types in QinQ tag. Current supported 
              types are CoS, Ethernet protocol type, VLAN ID and 
              Ethernet protocol type.
              
              Each bit field of zxDslEtherProtocolType has its 
              definitions such as follows.
        
            
           |    Byte 4    |    Byte 2    |    Byte 1    |    Byte 0    |
           |--------------|--------------+------|-------|--------------|
           |   Reserved   |     VLAN ID         |  Ether Type  |  CoS  |
           |    8 bits    |     12 bits         |    8 bits    | 4 bits|
           |--------------|--------------+------|-------|--------------|
           
           ** CoS :         0 - 7   
                            0xF indicates this field is not available(NA).
           
           ** Ethernet Protocol Type:
                            IP(1),
                            APP(2),
                            PPPOE_discovery(3),
                            PPPOE_session(4),
                            IPv6(5)
                            0xFF indicates this field is not available(NA).
                            
           ** VLAN ID:      1 - 4094 
                            0xFFF indicates this field is not available(NA).
           
           ** Reserved:     default is 0x00.  "
        ::= { zxDslProtocolVlanMapEntry 1 }

    zxDslProtocolSVlanId OBJECT-TYPE
        SYNTAX      Integer32 (1..4094) 
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "SVlanId ."
        ::= { zxDslProtocolVlanMapEntry 2 }
    
    zxDslProtocolSVlanPriority OBJECT-TYPE
        SYNTAX      Integer32 (0..7) 
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "SVlan cos priority"
        ::= { zxDslProtocolVlanMapEntry 3 }
        
     zxDslProtocolCVlanId OBJECT-TYPE
        SYNTAX      Integer32 (0..4094) 
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "CVlanId."
        ::= { zxDslProtocolVlanMapEntry 4 }
    
     zxDslProtocolCVlanPriority OBJECT-TYPE
        SYNTAX      Integer32 (0..7) 
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "CVlan cos priority"
        ::= { zxDslProtocolVlanMapEntry 5 }

    zxDslProtocolVlanRowStatus OBJECT-TYPE
        SYNTAX      RowStatus
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This object indicates the status of this entry."
        ::= { zxDslProtocolVlanMapEntry 100 }    

