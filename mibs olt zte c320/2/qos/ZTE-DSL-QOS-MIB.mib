    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION   "The CoS value of VoIP signaling."
    ::=  {  zxDslQosGlobalTable  5  }
  
  zxDslQosNetWorkSideQueueBufferSize  OBJECT-TYPE 
    SYNTAX      Integer32  ( 64 .. 128  ) 
    UNITS       "kbytes"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION   "The queue buffer size of networkside port."                   
    DEFVAL       { 64 }
    ::=  {  zxDslQosGlobalTable  6  }
    
  zxDslQosUserSideQueueBufferSize  OBJECT-TYPE 
