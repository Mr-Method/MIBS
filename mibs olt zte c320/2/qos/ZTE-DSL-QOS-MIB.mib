  zxDslQosSchedulerTable  OBJECT-TYPE
    SYNTAX    SEQUENCE  OF  zxDslQosSchedulerEntry
    MAX-ACCESS  not-accessible
    STATUS    current
    DESCRIPTION "The Scheduler Table enumerates packet schedulers. with each
            algorithm described by one diffServSchedulerEntry."
    ::=  { zxDslQosScheduler  1 }

  zxDslQosSchedulerEntry  OBJECT-TYPE
    SYNTAX    zxDslQosSchedulerEntry
    MAX-ACCESS  not-accessible
    STATUS    current
    DESCRIPTION "An entry in the Scheduler Table describing a single instance of
             a scheduling algorithm."
    INDEX   {  zxDslQosSchedulerID  }
    ::=  { zxDslQosSchedulerTable 1 }

  zxDslQosSchedulerEntry  ::=  SEQUENCE {
    zxDslQosSchedulerID  Integer32,
    zxDslQosSchedulerType  Integer32,
    zxDslQosSchedulerWRRWFQParam1  Integer32,
    zxDslQosSchedulerWRRWFQParam2  Integer32,
    zxDslQosSchedulerWRRWFQParam3  Integer32,
    zxDslQosSchedulerWRRWFQParam4  Integer32,
    zxDslQosSchedulerWRRWFQParam5  Integer32,
    zxDslQosSchedulerWRRWFQParam6  Integer32,
    zxDslQosSchedulerWRRWFQParam7  Integer32,
    zxDslQosSchedulerWRRWFQParam8  Integer32
    }


  zxDslQosSchedulerID OBJECT-TYPE 
    SYNTAX      INTEGER  { NNI ( 0 ) , UNI ( 1 ) , UNI-ETCA ( 2 ) } 
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION   
        "An index that enumerates the Scheduler entries,NNI means Network Node Interface,
        UNI meas User Network Interface,UNI-ETCA meas User Network Interface for Ethernet card.
        compatibility refrence zxDslSysCompatible of ZTE-DSL-SYSCTRL-MIB."
    ::=  {  zxDslQosSchedulerEntry  1  }


  zxDslQosSchedulerType OBJECT-TYPE 
    SYNTAX      INTEGER  { SP ( 1 ) , WRR ( 2 ) , WFQ ( 3 ) , SPplusWRR ( 4 ) , SPplusWFQ ( 5 ) } 
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION   "The scheduling algorithm used by this Scheduler."
    ::=  {  zxDslQosSchedulerEntry  2  }


  zxDslQosSchedulerWRRWFQParam1 OBJECT-TYPE 
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION   "Param1 of WRRWEQ"
    ::=  {  zxDslQosSchedulerEntry  3  }


  zxDslQosSchedulerWRRWFQParam2 OBJECT-TYPE 
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION   "Param2 of WRRWEQ"
    ::=  {  zxDslQosSchedulerEntry  4  }


  zxDslQosSchedulerWRRWFQParam3 OBJECT-TYPE 
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION   "Param3 of WRRWEQ"
    ::=  {  zxDslQosSchedulerEntry  5  }


  zxDslQosSchedulerWRRWFQParam4 OBJECT-TYPE 
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION   "Param4 of WRRWEQ"
    ::=  {  zxDslQosSchedulerEntry  6  }


  zxDslQosSchedulerWRRWFQParam5 OBJECT-TYPE 
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION   "Param5 of WRRWEQ"
    ::=  {  zxDslQosSchedulerEntry  7  }


  zxDslQosSchedulerWRRWFQParam6 OBJECT-TYPE 
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION   "Param6 of WRRWEQ"
    ::=  {  zxDslQosSchedulerEntry  8  }


  zxDslQosSchedulerWRRWFQParam7 OBJECT-TYPE 
    SYNTAX      Integer32
    MAX-ACCESS  read-write
