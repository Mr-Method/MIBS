        REFERENCE   "G.983.2 section 7.3.49 VLAN tagging operation configuration data"
        ::= { zxGponIPHostVLANTagOperConfigEntry 3 }
        
    zxGponIPHostVLANTagOperConfigEntryStatus OBJECT-TYPE
